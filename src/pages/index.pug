    body
        section.header
            .container.header-container     
                nav.row
                    .d-flex.col-lg-7.col-md-12.col-sm-12.justify-content-between.align-items-center
                        a(href='https://krayt.ru/').header__logo-container
                            img(src='./assets/img/Logo.png' class='header__logo')
                            span.header__logo-text Giant 
                        i.fas.fa-bars.menu-bar      
                    .col-lg-5.header__nav
                        a(href='https://krayt.ru/').header__link Home
                        a(href='https://krayt.ru/').header__link Future
                        a(href='https://krayt.ru/').header__link Pricing
                        form(action="https://krayt.ru/" target="_blank")
                            +button('Download Now')
                    .header.header__burger.col-12
                        a(href='https://krayt.ru/').header__link Home
                        a(href='https://krayt.ru/').header__link Future
                        a(href='https://krayt.ru/').header__link Pricing
                        form(action="https://krayt.ru/" target="_blank")
                            +button('Download Now')            
                img(src='./assets/img/iPhones.png' class='header__background-image')                
                main.header__main.row.col-lg-6.col-md-12.flex-column
                    h1.header__title A Powerful & Unique App Landing Template
                    .header__main-content Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.
                    .main__item-container
                        +list-item({listText:'Unlimited colors: banner with images, slider or video',
                                    listClass:'wight',
                                    listTextClass:'wight'})
                    .main__item-container                    
                        +list-item({listText:'Fully Responsive and powered by HTML5 and CSS3',
                                    listClass:'wight',
                                    listTextClass:'wight'})                
                    +list-item({listText:'Very easy to customize : set up in minutes',
                                    listClass:'wight',
                                    listTextClass:'wight'}) 
                    .header__main-button
                        +button('Start Exploring Us')
        section.features
            .container
                .row    
                    +features-item({iconClass:'fa-tags', title:'Sell any product online',text:'Over 92% of computers are infected with Adware and spyware.Such software is'})
                    +features-item({iconClass:'fa-cog', title:'Very easy to customize',text:'Many of us are so used to working on a computer desktop that when it comes'})
                    +features-item({iconClass:'fa-paint-brush', title:'Unlimited colors',text:'Audio player software is used to play back sound recordings in one of the'})
                    +features-item({iconClass:'fa-fire-extinguisher', title:'Responsive, HTML5, CSS3',text:'Computers have become ubiquitous in almost every facet of our lives. At work,'})
                    +features-item({iconClass:'fa-file-alt', title:'Designed to convert',text:'If you are looking for a new way to promote your business that won’t cost'})
                    +features-item({iconClass:'fa-life-ring', title:'With no boundaries',text:'Now, if you are interested in being the best player, getting really good money'})
        section.dont-leave
            .container.dont-leave__container
                .row.d-flex.flex-sm-wrap   
                    img(src="./assets/img/Device.png", alt="").dont-leave__img.col-lg-6.col-md-12.col-sm-12  
                    .dont-leave__content.col-lg-6.col-md-12.col-sm-12 
                        h2 We don’t leave you stranded
                        p.dont-leave__title-description Everyone’s reasons for learning user interface design are different. If you’re already a developer, or a PM, or a UX designer, why develop this totally separate skill?
                        p.dont-leave__main-text Everyone’s reasons for learning user interface design are different. If you’re already a developer, or a PM, or a UX designer, why develop this totally separate skill? If you’re already a developer, or a PM, or a UX designer.
                        .dont-leave__list
                            .main__item-container
                                +list-item({listText:'Unlimited colors: banner with images, slider or video',
                                        listClass:'black',
                                        listTextClass:'black'})
                            .main__item-container            
